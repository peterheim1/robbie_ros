<launch>
<!-- used to launch all working action servers-->



    
     <!-- Head action server  -->
    <node name="robbie_head_action" pkg="robbie_bringup" type="follow_head_gazebo.py" output="screen"/>

  <!-- Arm action server  -->
    
    <node name="right_robbie_arm_action" pkg="robbie_bringup" type="right_follow_joint.py" output="screen"/>
    <node name="left_robbie_arm_action" pkg="robbie_bringup" type="left_follow_joint.py" output="screen"/>

  <node name="right_gripper" pkg="robbie_bringup" type="gripper_controller_gazebo.py"/>
    	<param name="model" value="singlesided"/>

  <node name="left_gripper" pkg="robbie_bringup" type="left_gripper_controller_gazebo.py"/>
    	<param name="model" value="singlesided"/>

</launch>
